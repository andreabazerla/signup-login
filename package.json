{
  "name": "affittagram",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Andrea Bazerla",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node ./dist/api/app.js",
    "pre-dev": "tsc -p api",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p api\" \"nodemon ./dist/api/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p api && node ./dist/api/app.js\"",
    "build": "ng build",
    "build-production": "ng build --aot --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "concurrently \"ng build --prod\" \"tsc -p api\" "
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/cli": "^10.0.8",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@auth0/angular-jwt": "^5.0.1",
    "angular-jwt": "^0.1.11",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.2",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.1",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.3",
    "concurrently": "^5.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.55",
    "codelyzer": "^6.0.0",
    "debug": "^4.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0"
  }
}
